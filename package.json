{
	"name": "obsidian-knowntags",
	"version": "1.0.13",
	"description": "Known Tags Plugin for Obsidian (https://obsidian.md)",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/language": "^6.4.0",
		"@codemirror/view": "^6.7.3",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"tsup": "^6.6.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"file-type": "^18.2.0",
		"got": "^12.5.3",
		"openai": "^3.1.0"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"interface": "npm run build && tsup && npm pack"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"tsup": {
		"entry": [
			"./index.ts"
		],
		"splitting": false,
		"sourcemap": false,
		"clean": true,
		"dts": true
	},
	"files": [
		"dist/index.d.ts",
		"dist/index.js",
		"manifest.json",
		"README.md",
		"LICENSE",
		"CHANGELOG.md",
		"versions.json"
	]
}
